{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" }
      ]
    },
    {
      "source": "/og/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" }
      ]
    }
  ],
  "rewrites": [
  { "source": "/plans", "destination": "/index.html" },
  { "source": "/services", "destination": "/index.html" },
  { "source": "/services/websites", "destination": "/index.html" },
  { "source": "/services/social-media", "destination": "/index.html" },
  { "source": "/services/branding", "destination": "/index.html" },
  { "source": "/portfolio", "destination": "/index.html" },
  { "source": "/about", "destination": "/index.html" },
  { "source": "/order/summary", "destination": "/index.html" },
  { "source": "/order/details", "destination": "/index.html" },
  { "source": "/order/success", "destination": "/index.html" },
  { "source": "/articles/:slug*", "destination": "/index.html" },
  { "source": "/:path*", "destination": "/index.html" }
  ]
}